            <string>///ZOOM
if obj_global_handle.camera == 1 {
if players &gt;= 2 //Skips the camera zoom code for 1 test
{
    if players == 2
    {
        p1x = obj_test0.x // gather x/y positions for 2 players mode
        p1y = obj_test0.y
        p2x = obj_test1.x
        p2y = obj_test1.y

        if p1x &gt; p2x {largex = obj_test0} //determine lowest x/y and highest x/y for 2 players mode
        else if p2x &gt; p1x {largex = obj_test1} //also setting variable largex/y/smallx/y to the test fathest away

        if p1x &lt; p2x {smallx = obj_test0}
       else if p2x &lt; p1x {smallx = obj_test1}

       if p1y &gt; p2y {largey = obj_test0}
       else if p2y &gt; p1y {largey = obj_test1}

       if p1y &lt; p2y {smally = obj_test0}
       else if p2y &lt; p1y {smally = obj_test1}
   }
   if players == 3
   {
       p1x = obj_test0.x // gather x/y positions for 3 players mode
       p1y = obj_test0.y
       p2x = obj_test1.x
       p2y = obj_test1.y
       p3x = obj_test2.x
       p3y = obj_test2.y

       if p1x &gt; p2x and p1x &gt; p3x{largex = obj_test0} //determine lowest x/y and highest x/y for 3 players mode
       else if p2x &gt; p1x and p2x &gt; p3x{largex = obj_test1}//variable largex/y/smallx/y to the test fathest away
       else if p3x &gt; p1x and p3x &gt; p2x{largex = obj_test2}

       if p1x &lt; p2x and p1x &lt; p3x{smallx = obj_test0}
       else if p2x &lt; p1x and p2x &lt; p3x {smallx = obj_test1}
       else if p3x &lt; p1x and p3x &lt; p2x {smallx = obj_test2}

       if p1y &gt; p2y and p1y &gt; p3y {largey = obj_test0}
       else if p2y &gt; p1y and p2y &gt; p3y{largey = obj_test1}
       else if p3y &gt; p1y and p3y &gt; p2y{largey = obj_test2}

       if p1y &lt; p2y and p1y &lt; p3y {smally = obj_test0}
       else if p2y &lt; p1y and p2y &lt; p3y {smally = obj_test1}
       else if p3y &lt; p1y and p3y &lt; p2y {smally = obj_test2}
}
if players == 4
{
       p1x = obj_test0.x // gather x/y positions for 4 players mode
       p1y = obj_test0.y
       p2x = obj_test1.x
       p2y = obj_test1.y
       p3x = obj_test2.x
       p3y = obj_test2.y
       p4x = obj_test3.x
       p4y = obj_test3.y

if p1x &gt; p2x and p1x &gt; p3x and p1x &gt; p4x{largex = obj_test0} //lowest x/y and highest x/y for 4 players mode
       else if p2x &gt; p1x and p2x &gt; p3x and p2x &gt; p4x{largex = obj_test1}// largex/y/smallx/y to the test fathest away
       else if p3x &gt; p1x and p3x &gt; p2x and p3x &gt; p4x{largex = obj_test2}
       else if p4x &gt; p1x and p4x &gt; p3x and p4x &gt; p2x{largex = obj_test3}

       if p1x &lt; p2x and p1x &lt; p3x and p1x &lt; p4x{smallx = obj_test0}
       else if p2x &lt; p1x and p2x &lt; p3x and p2x &lt; p4x{smallx = obj_test1}
       else if p3x &lt; p1x and p3x &lt; p2x and p3x &lt; p4x{smallx = obj_test2}
       else if p4x &lt; p1x and p4x &lt; p3x and p4x &lt; p2x{smallx = obj_test3}

       if p1y &gt; p2y and p1y &gt; p3y and p1y &gt; p4y{largey = obj_test0}
       else if p2y &gt; p1y and p2y &gt; p3y and p2y &gt; p4y{largey = obj_test1}
       else if p3y &gt; p1y and p3y &gt; p2y and p3y &gt; p4y{largey = obj_test2}
       else if p4y &gt; p1y and p4y &gt; p3y and p4y &gt; p2y{largey = obj_test3}

       if p1y &lt; p2y and p1y &lt; p3y and p1y &lt; p4y{smally = obj_test0}
       else if p2y &lt; p1y and p2y &lt; p3y and p2y &lt; p4y{smally = obj_test1}
       else if p3y &lt; p1y and p3y &lt; p2y and p3y &lt; p4y{smally = obj_test2}
       else if p4y &lt; p1y and p4y &lt; p3y and p4y &lt; p2y{smally = obj_test3}
}

distx = (largex.x - smallx.x); camPlayX = smallx.x //get distance between the farthest objects 
disty = (largey.y - smally.y); camPlayY = smally.y //and tell camera which object to follow

dist12x = distx
dist12y = disty
// X:850 0.001150      Y: 550 0.001800
if dist12x &gt; 550 or dist12y &gt; 550 //when the players get 650px apart begin to zoom out
{
    if dist12x &gt; dist12y{var_zoom = lerp(var_zoom,(dist12x * 0.001800),0.5)}   //smooth zoom using LERP
    else if dist12x &lt; dist12y{var_zoom = lerp(var_zoom,(dist12y * 0.001800),0.5)}

    view_hview[0] = floor(hviewStart * var_zoom)  //adjust view to create zoom
    view_wview[0] = floor(wviewStart * var_zoom)
}
else
{
    view_hview[0] = hviewStart  //zoom default
    view_wview[0] = wviewStart
}
movex =  (camPlayX)+ dist12x *0.5  //center the view from the smallest x/y object + half way to the farthest object
movey = (camPlayY)+ dist12y *0.5
dist = distance_to_point(movex,movey)
spd = lerp(0,dist,0.12)
move_towards_point(movex,movey,spd)  //move the camera
}
else if players == 1  //simple camera movement with lerp,  no zoom
{
  movex =  obj_test0.x
  movey = obj_test0.y
  dist = distance_to_point(movex,movey)
  spd = lerp(0,dist,0.12)
  move_towards_point(movex,movey,spd)

  view_hview[0] = hviewStart
  view_wview[0] = wviewStart
}
}

